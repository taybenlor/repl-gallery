---
pagination:
  data: projects
  size: 1
  alias: project
permalink: "projects/{{ project.author_name }}/{{ project.id }}/index.html"
---

<!doctype html>
<html class="no-js h-full" lang="en">
<head>
  <meta charset="utf-8">
  <title>{{ project.title }} by {{ project.author_name }}</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="theme-color" content="#ffffff">
  <link href="../../../style.css" rel="stylesheet">
</head>

<body class="flex flex-col h-full">
  <header class="bg-blue-800 py-4">
    <div class="container mx-auto">
      <a class="text-white font-bold" href="../../../index.html">
        Back to Gallery
      </a>
    </div>
  </header>
  <main class="flex-grow">
    <iframe src="{{ project.url }}&embed=true" sandbox="allow-forms allow-pointer-lock allow-popups allow-same-origin allow-scripts" frameborder="0" width="100%" height="100%"></iframe>
  </main>
</body>

</html>